    "@types/lodash": "^4.14.186",
    "classnames": "^2.3.2",
    "feed": "^4.2.2",
    "framer-motion": "^7.6.1",
    "lodash": "^4.17.21",
    "next": "^12.3.1",
    "next-themes": "^0.2.1",
    "nextjs-progressbar": "0.0.16",
    "react": "18.2.0",
    "react-countup": "^6.3.2",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^9.4.0",
    "react-intl": "^6.2.1",
